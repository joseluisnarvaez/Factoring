	<groupId>cl.starttoken</groupId>
	<artifactId>Factoring</artifactId>
	<packaging>war</packaging>
	<version>1.0.0</version>
	<name>Factoring Maven Webapp</name>
	<url>http://maven.apache.org</url>

	<properties>
